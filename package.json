{
  "name": "rnts",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky install",
    "lint:fix": "eslint ./src --fix",
    "lint:fix-ff": "npx eslint . --fix",
    "node:clean": "rm -rf node_modules&& yarn cache clean && yarn install",
    "resetc": "yarn start -- --reset-cache",
    "tron": "adb reverse tcp:9090 tcp:9090",
    "adb-r": "adb reverse tcp:8081 tcp:8081",
    "adb-start": "adb reverse tcp:9090 tcp:9090 && node node_modules/react-native/local-cli/cli.js start",
    "setDevEnv": "ENVFILE=.env.dev",
    "setQaEnv": "ENVFILE=.env.qa",
    "setStagingEnv": "ENVFILE=.env.staging",
    "setProdEnv": "ENVFILE=.env.production",
    "android:dev": "yarn react-native run-android --mode=devDebug --appIdSuffix=Development",
    "android:dev-aab": "cd android && ./gradlew bundleDevRelease",
    "android:dev-apk": "cd android && setDevEnv ./gradlew assembleRelease",
    "android:qa": "yarn react-native run-android --mode=qaDebug --appIdSuffix=qa",
    "android:qa-aab": "cd android && ./gradlew bundleqaRelease",
    "android:qa-apk": "cd android && setQaEnv ./gradlew assembleRelease",
    "android:staging": "yarn react-native run-android --mode=stagingDebug --appIdSuffix=staging",
    "android:staging-aab": "cd android && ./gradlew bundlestagingRelease",
    "android:staging-apk": "cd android && setStagingEnv ./gradlew assembleRelease",
    "android:prod": "yarn react-native run-android --mode=prodDebug --appIdSuffix=prod",
    "android:prod-aab": "cd android && ./gradlew bundleprodRelease",
    "android:prod-apk": "cd android && setProdEnv ./gradlew assembleRelease",
    "android:clean": "cd android && ./gradlew clean",
    "android:clean-all": "yarn node:clean && cd android && ./gradlew clean",
    "ios:pod-reset": "cd ios && pod deintegrate && pod setup && pod install && rm -rf .xcode.env.local",
    "ios:pod-install": "cd ios && pod install && rm -rf .xcode.env.local",
    "ios:clean": "yarn node:clean &&  cd ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && rm -rf Podfile.lock && yarn ios:pod-reset",
    "ios:dev": "yarn react-native run-ios --mode=Debug --scheme \"Development\"",
    "ios:dev-ipa": "yarn ios:clean && yarn setDevEnv react-native run-ios --mode=Release --scheme \"Development\"",
    "ios:qa": "yarn setQaEnv react-native run-ios --mode=Debug --scheme \"Qa\"",
    "ios:qa-ipa": "yarn ios:clean && yarn setQaEnv react-native run-ios --mode=Release --scheme \"Qa\"",
    "ios:staging": "yarn setStagingEnv react-native run-ios --mode=Debug --scheme \"Staging\"",
    "ios:staging-ipa": "yarn ios:clean && yarn setStagingEnv react-native run-ios --mode=Release --scheme \"Staging\"",
    "ios:prod": "yarn setProdEnv react-native run-ios --mode=Debug --scheme \"Production\"",
    "ios:prod-ipa": "yarn ios:clean && yarn setProdEnv react-native run-ios --mode=Release --scheme \"Production\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-config": "^1.5.1",
    "react-native-reanimated": "^3.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@tsconfig/react-native": "^3.0.3",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.55",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.3",
    "lint-staged": "^15.2.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.1.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": "eslint"
  },
  "compilerOptions": {
    "moduleResolution": "node"
  }
}
